<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.heathendean.catfarts.settings.Settings$PlaceholderFragment">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/app_id"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/version_number"/>
		<Button
			android:id="@+id/testErrorButton"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:onClick="test_error"
			android:text="@string/text_test_error_button" />
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="horizontal">
			<Button
				android:id="@+id/testWebErrorButton"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:onClick="test_web_error"
				android:text="@string/text_test_web_error_button" />

			<WebView
				android:id="@+id/testWebErrorView"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"/>
		</LinearLayout>
	</LinearLayout>
	<com.heathendean.catfarts.custom.customImageButton
		android:id="@+id/backHomeFromSettings"
		android:layout_width="200dp"
		android:layout_height="200dp"
		android:layout_alignParentBottom="true"
		android:adjustViewBounds="false"
		android:background="@drawable/back_triangle"
		android:onClick="back_to_settings"
		android:contentDescription="@string/desc"/>
</RelativeLayout>
