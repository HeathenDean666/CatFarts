<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.heathendean.catfarts.settings.Settings$PlaceholderFragment">

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Upload Cat"
			android:onClick="upload_fart"/>
		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="View Cat Gallery"
			android:onClick="view_fart_gallery"/>
	</LinearLayout>

	<com.heathendean.catfarts.custom.customImageButton
		android:id="@+id/backHomeFromSettings"
		android:layout_width="200dp"
		android:layout_height="200dp"
		android:layout_alignParentBottom="true"
		android:adjustViewBounds="false"
		android:background="@drawable/back_triangle"
		android:onClick="back_to_settings"
		android:contentDescription="@string/desc"/>

</RelativeLayout>
