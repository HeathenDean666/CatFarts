<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.heathendean.catfarts.settings.Settings$PlaceholderFragment">

	<WebView
		android:id="@+id/loreWebView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"/>

	<com.heathendean.catfarts.custom.customImageButton
		android:id="@+id/backHomeFromSettings"
		android:layout_width="200dp"
		android:layout_height="200dp"
		android:layout_alignParentBottom="true"
		android:adjustViewBounds="false"
		android:background="@drawable/back_triangle"
		android:onClick="back_to_settings"
		android:contentDescription="@string/desc"/>

</RelativeLayout>
