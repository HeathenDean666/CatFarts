<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.heathendean.catfarts.settings.Settings$PlaceholderFragment">

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="match_parent">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical" >

			<ImageButton
				android:id="@+id/imageButton1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:onClick="cat_options"
				android:src="@drawable/button_image_01"
				android:tint="@null" />

			<ImageButton
				android:id="@+id/imageButton2"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:onClick="fart_options"
				android:src="@drawable/button_image_01"
				android:tint="@null" />

			<ImageButton
				android:id="@+id/imageButton3"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:src="@drawable/button_image_01"
				android:tint="@null" />

			<ImageButton
				android:id="@+id/imageButton4"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:src="@drawable/button_image_01"
				android:tint="@null" />

			<ImageButton
				android:id="@+id/imageButton5"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:src="@drawable/button_image_01"
				android:tint="@null" />

			<ImageButton
				android:id="@+id/imageButton6"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@null"
				android:backgroundTint="@null"
				android:contentDescription="@string/settings_imageButton_01_contentDescription"
				android:src="@drawable/button_image_01"
				android:tint="@null" />
		</LinearLayout>
	</ScrollView>

	<ImageButton
		android:id="@+id/imageButton7"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:adjustViewBounds="false"
		android:background="@null"
		android:backgroundTint="@null"
		android:contentDescription="@string/settings_imageButton_01_contentDescription"
		android:onClick="back_to_main"
		android:src="@drawable/button_image_02"
		android:tint="@null" />

</RelativeLayout>
