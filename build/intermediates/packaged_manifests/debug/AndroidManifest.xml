<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.heathendean.catfarts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="30" />

    <application
        android:name="com.heathendean.catfarts.CatFarts"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@drawable/app_icon"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.heathendean.catfarts.MainActivity"
            android:label="@string/app_name" >

            <!-- android:label="@string/title_activity_main" > -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.heathendean.catfarts.play.Play"
            android:label="@string/title_activity_start_game"
            android:parentActivityName="com.heathendean.catfarts.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.Settings"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.heathendean.catfarts.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <!-- Settings Activities -->
        <activity
            android:name="com.heathendean.catfarts.settings.instructions.Instructions"
            android:label="@string/title_activity_instructions"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.sound.Sound"
            android:label="@string/title_activity_sound"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.pictures.Pictures"
            android:label="@string/title_activity_pictures"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.gameplay.Gameplay"
            android:label="@string/title_activity_gameplay"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.options.Options"
            android:label="@string/title_activity_options"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.networking.Networking"
            android:label="@string/title_activity_networking"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.cheats.Cheats"
            android:label="@string/title_activity_cheats"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.contact.Contact"
            android:label="@string/title_activity_contact"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.settings.lore.Lore"
            android:label="@string/title_activity_lore"
            android:parentActivityName="com.heathendean.catfarts.settings.Settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="cat.ereza.customactivityoncrash.activity.DefaultErrorActivity"
            android:process=":error_activity" />

        <provider
            android:name="cat.ereza.customactivityoncrash.provider.CaocInitProvider"
            android:authorities="com.heathendean.catfarts.customactivityoncrashinitprovider"
            android:exported="false"
            android:initOrder="101" />
    </application>

</manifest>