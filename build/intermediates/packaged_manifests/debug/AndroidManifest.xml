<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.heathendean.catfarts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name="com.heathendean.catfarts.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.heathendean.catfarts.StartGame"
            android:label="@string/title_activity_start_game"
            android:parentActivityName="com.heathendean.catfarts.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.Commands"
            android:label="@string/title_activity_commands"
            android:parentActivityName="com.heathendean.catfarts.Commands" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.Commands" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.Settings"
            android:label="@string/title_activity_start_game"
            android:parentActivityName="com.heathendean.catfarts.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
        <activity
            android:name="com.heathendean.catfarts.Contact"
            android:label="@string/title_activity_start_game"
            android:parentActivityName="com.heathendean.catfarts.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.heathendean.catfarts.MainActivity" />
        </activity>
    </application>

</manifest>